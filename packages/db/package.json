{
  "name": "@ctc-cms/db",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "sst bind vitest",
    "typecheck": "tsc -noEmit",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./scripts/clear.ts",
    "db:seed": "tsx ./scripts/seed.ts",
    "db:init": "npm run db:clear && npm run db:seed"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "sst": "^2.43.3",
    "tsx": "^4.16.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "drizzle-orm": "^0.31.2",
    "postgres": "^3.4.4"
  }
}
